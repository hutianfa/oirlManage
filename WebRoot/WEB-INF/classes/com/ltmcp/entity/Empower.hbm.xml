<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ltmcp.entity.Empower" table="empower">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="nm"/>
        </property>
        <property name="powerCode" type="java.lang.String">
            <column name="powercode"/>
        </property>
        
        <property name="createName" type="java.lang.String">
            <column name="create_name"/>
        </property>
        <property name="company" type="java.lang.Integer">
            <column name="com" />
        </property>
    </class>
</hibernate-mapping>
