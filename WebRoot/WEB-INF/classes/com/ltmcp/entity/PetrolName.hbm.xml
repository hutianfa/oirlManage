<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ltmcp.entity.PetrolName" table="petrol_infor" lazy="true">
        
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        
        <property name="you_nfc_id" type="java.lang.String">
            <column name="you_nfc_id" />
        </property> 
        
        <property name="you_name" type="java.lang.String">
            <column name="you_name" />
        </property> 
        
        <property name="com_id" type="java.lang.Integer">
            <column name="com_id" />
        </property>      
    </class>
</hibernate-mapping>
