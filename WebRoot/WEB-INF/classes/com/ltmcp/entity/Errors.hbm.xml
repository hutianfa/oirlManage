<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.ltmcp.entity.Errors" table="errors">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        
        <property name="time" type="java.sql.Timestamp">
            <column name="time"/>
        </property>
        <property name="code" type="java.lang.String">
            <column name="code" />
        </property>
        <property name="returnNum" type="java.lang.String">
            <column name="re" />
        </property>
        
        <property name="username" type="java.lang.String">
            <column name="username" />
        </property>
        
        <property name="posid" type="java.lang.String">
            <column name="posid" />
        </property>
        
        <property name="appNum" type="java.lang.String">
            <column name="appNum" />
        </property>
 
    </class>
</hibernate-mapping>
